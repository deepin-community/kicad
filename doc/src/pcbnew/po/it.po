"    0 -0.5 8 0\n"
"    0 -0.5 0.5 0\n"
"    0 -2.5 0.5 0\n"
"    0 -2.5 -0.5 180\n"
"    0 2.5 -0.5 0\n"
"    0 2.5 0.5 180\n"
"    0 0.5 0.5 0\n"
