"  <node ref=\"U1\" pin=\"7\"/>\n"
"  <node ref=\"C1\" pin=\"2\"/>\n"
"  <node ref=\"U2\" pin=\"7\"/>\n"
"  <node ref=\"P1\" pin=\"4\"/>\n"
