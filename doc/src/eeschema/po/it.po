"                    <!-- if a net has a name, use it,\n"
"                        else build a name from its net code\n"
"                    -->\n"
"                    <xsl:when test = \"../@name != '' \">\n"
"                        <xsl:value-of select=\"../@name\"/>\n"
"                    </xsl:when>\n"
"                    <xsl:otherwise>\n"
"                        <xsl:text>$N-0</xsl:text><xsl:value-of select=\"../@code\"/>\n"
"                    </xsl:otherwise>\n"
"                </xsl:choose>\n"
"            </xsl:if>\n"
"            <xsl:if test = \"$pin_cnt_in_net &lt;2\">\n"
"                <xsl:text>?</xsl:text>\n"
"            </xsl:if>\n"
"        </xsl:if>\n"
"    </xsl:if>\n"

