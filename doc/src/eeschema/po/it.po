"                    </xsl:when>\n"
"                    <xsl:otherwise>\n"
"                        <xsl:text>$N-0</xsl:text><xsl:value-of select=\"../@code\"/>\n"
"                    </xsl:otherwise>\n"
"                </xsl:choose>\n"
"            </xsl:if>\n"
"            <xsl:if test = \"$pin_cnt_in_net &lt;2\">\n"
"                <xsl:text>?</xsl:text>\n"
"            </xsl:if>\n"
"        </xsl:if>\n"
"    </xsl:if>\n"
msgstr ""
"    <xsl:if test = \"@ref = $cmp_ref \">\n"
"        <xsl:if test = \"@pin = $pin_num\">\n"
"        <!-- net name is output only if there is more than one pin in net\n"
